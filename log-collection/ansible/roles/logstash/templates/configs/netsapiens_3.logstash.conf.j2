input {
	jdbc {
		jdbc_driver_library => "/usr/share/java/mysql-connector-java-8.0.19.jar"
		jdbc_driver_class => "com.mysql.jdbc.Driver"

		jdbc_connection_string => "{{ netsapiens_3.connection_string }}"
		jdbc_user => "{{ netsapiens_3.user}}"
		jdbc_password => "{{ netsapiens_3.password }}"
                jdbc_paging_enabled => true
                jdbc_page_size => "{{ jdbc_page_size }}"

                last_run_metadata_path => "/etc/logstash/conf.d/.logstash_jdbc_last_run_netsapiens_3"
                statement_filepath => "/etc/logstash/conf.d/netsapiens_mysql_statement"
                schedule => "*/1 * * * *"
                use_column_value => true
                tracking_column => "event_index"
	}
}

filter {
  mutate {
    rename => {
      "orig_callid" => "call-id"
    }
    rename => {
      "event_text" => "message"
    }
    add_field => {
      "capture.src.ip" => "{{ netsapiens_3.nsserver_ip }}"
      "capture.src.port" => "{{ netsapiens_3.nsserver_port }}"
    }
  }
  date {
    match => ["event_ts", "UNIX_MS"]
  }
}

output {
    elasticsearch {
        hosts => "{{ dashbase_url }}"
        index => "{{ table }}"
        ssl_certificate_verification => false
    }
}
